{
  "containers": {
    "mappings": {
      "dynamic_templates": [
        {
          "strings": {
            "mapping": {
              "copy_to": "text_fields",
              "fields": {
                "keyword": {
                  "ignore_above": 256,
                  "type": "keyword"
                }
              },
              "norms": false,
              "type": "text"
            },
            "match": "*",
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "_timestamp": {
          "store": true,
          "type": "date"
        },
        "digest": {
          "copy_to": [
            "text_fields"
          ],
          "fields": {
            "keyword": {
              "ignore_above": 256,
              "type": "keyword"
            }
          },
          "norms": false,
          "type": "text"
        },
        "id": {
          "copy_to": [
            "text_fields"
          ],
          "fields": {
            "keyword": {
              "ignore_above": 256,
              "type": "keyword"
            }
          },
          "norms": false,
          "type": "text"
        },
        "text_fields": {
          "type": "text"
        },
        "url": {
          "copy_to": [
            "text_fields"
          ],
          "fields": {
            "keyword": {
              "ignore_above": 256,
              "type": "keyword"
            }
          },
          "norms": false,
          "type": "text"
        },
        "version": {
          "copy_to": [
            "text_fields"
          ],
          "fields": {
            "keyword": {
              "ignore_above": 256,
              "type": "keyword"
            }
          },
          "norms": false,
          "type": "text"
        }
      }
    }
  }
}