{
    "command": "/home/ops/lightweight-jobs/wget.sh",
    "container": "container-hysds_lightweight-jobs:v1.0.6",
    "disk_usage": "3GB",
    "id": "job-lw-tosca-wget:v1.0.6",
    "imported_worker_files": {
        "$HOME/.aws": "/home/ops/.aws"
    },
    "job-version": "v1.0.6",
    "params": [
        {
            "destination": "positional",
            "name": "query"
        },
        {
            "destination": "positional",
            "name": "emails"
        },
        {
            "destination": "positional",
            "name": "name"
        }
    ],
    "required_queues": [
        "system-jobs-queue"
    ],
    "resource": "jobspec",
    "soft_time_limit": 86400,
    "time_limit": 86700
}