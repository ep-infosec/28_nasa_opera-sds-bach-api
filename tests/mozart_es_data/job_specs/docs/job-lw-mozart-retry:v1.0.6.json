{
    "command": "/home/ops/lightweight-jobs/retry.sh",
    "container": "container-hysds_lightweight-jobs:v1.0.6",
    "disk_usage": "3GB",
    "id": "job-lw-mozart-retry:v1.0.6",
    "job-version": "v1.0.6",
    "params": [
        {
            "destination": "context",
            "name": "retry_job_id"
        },
        {
            "destination": "context",
            "name": "type"
        },
        {
            "destination": "context",
            "name": "retry_count_max"
        },
        {
            "destination": "context",
            "name": "job_priority_increment"
        }
    ],
    "required_queues": [
        "system-jobs-queue"
    ],
    "resource": "jobspec",
    "soft_time_limit": 86400,
    "time_limit": 86700
}